{
  "name": "websitebase",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel mobile backend-dev",
    "website-dev": "cd website && npm run dev",
    "backend-dev": "cd backend && npm run dev",
    "website-mobile-dev": "cd website && npm run mobile-dev -- --host",
    "mobile": "cross-env npm run website-mobile-dev",
    "build": "cd website && npm run deploy"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "ssh2-sftp-client": "^12.0.0"
  },
  "devDependencies": {
    "cross-env": "^10.1.0"
  }
}
